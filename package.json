  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "dependency-check": "^3.3.0",
    "hallmark": "^0.1.0",
    "level-community": "^3.0.0",
    "memdown": "^1.2.4",
    "nyc": "^12.0.2",
    "standard": "^11.0.1",
    "tape": "^4.6.3"
  },
  "hallmark": {
    "community": "level-community",
    "ignore": [
      "README.md"
    ]
